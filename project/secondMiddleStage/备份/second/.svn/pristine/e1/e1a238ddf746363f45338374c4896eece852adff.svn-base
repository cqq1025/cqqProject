@import 'api';
@import 'animate';
@import 'iconfont';
$bgColor: #323232;
$lightColor: #ccc;
$titleColor: #455544;
//标题颜色--墨绿
$redColor: #9a321d;
//日落橙色
$blueColor: #383d51;
//午夜蓝
$greenColor: #7e834b;
//柑橘绿
$whiteColor: #fff;
//白色
$grayColor: #868c97;
//军校灰色
$fontSize: 2em;
//标题大小
$txtColor: #808080;
//内容文字颜色
$txtFontSize: 0.67em;
//内容文字大小
header {
    z-index: 1000;
    position: fixed;
    top: 1em;
    left: 1em;
    right: 1em;
    height: 3em;
    font-weight: 600;
    svg:hover {
        opacity: 0.6;
    }
    .coverTopRight {
        button {
            text-align: center;
            line-height: 2em;
            width: 6em;
            height: 2em;
            color: #fff;
            background: #4a4949;
            border-radius: 0.5em;
            outline: 0;
            cursor: pointer;
        }
        button:hover {
            background: #d3e1d5;
        }
    }
}
.cover {
    width: 100%;
    box-sizing: border-box;
    video {
        position: absolute;
        top: -1em;
        right: 0;
        width: 100%;
    }
    .coverBottom {
        position: relative;
        box-sizing: border-box; //      padding: 10em 5em;
        .coverBottomLeft {
            height: 500px;
            position: absolute;
            top: 0;
            left: 0;
            z-index: 1000;
            box-sizing: border-box;
            padding-top: 8em;
            padding-left: 5em;
            span {
                font-weight: 600;
                font-size: 2.5em;
            }
        }
        .coverBottomRight {
            .coverGo {
                z-index: 99999;
                position: absolute;
                top: 20em;
                left: 20em;
                text-align: center;
            }
        }
    }
}
.first {
    position: relative;
    * {
        color: $whiteColor;
    }
    video {
        display: none; //      position: absolute;left: 0;top: 0;
        width: auto;
        height: 100%;
    }
    video.active {
        display: block;
    }
    .one-word {
        z-index: 100;
        position: absolute;
        top: 10em;
        left: 12em;
        opacity: 0;
        transform: translate3d(0px, 50px, 0);
        h1 {
            font-size: 1.5em;
            margin-bottom: 0.4em;
        }
        p {
            margin-top: 0.1em;
            font-size: 0.8em;
        }
        span {
            margin-top: 0.8em;
            font-size: 1em;
            font-weight: 600;
        }
    }
    .two {
        position: absolute;
        top: 11em;
        left: 12em;
    }
    .three {
        position: absolute;
        top: 13em;
        left: 13em;
    }
    .one-word.active {
        transform: translate3d(0, 0, 0);
        transition: all 1s ease-in;
        opacity: 1;
    }
}
.secondApproach {
    overflow: hidden;
    background: $whiteColor;
    position: relative;
    .txtBox {
        z-index: 300;
        width: 100%;
        text-align: center;
        position: absolute;
        top: 5em;
        transform: translate3d(0, -10em, 0);
        h3 {
            color: $titleColor;
            font-size: $fontSize;
        }
        p {
            font-size: $txtFontSize;
            color: $txtColor;
            font-family: "微软雅黑";
            margin-top: 1em;
        }
    }
    .txtBox.active {
        transform: translate3d(0, 0em, 0);
        transition: transform 2s ease-in;
    }
    img {
        z-index: 200;
        position: absolute;
        bottom: -3em;
        width: 100%;
        transform: translate3d(0, 8em, 0);
    }
    img.active {
        transform: translate3d(0, 0, 0);
        transition: transform 2s ease-in;
    }
}
.thirdBox {
    overflow: hidden;
    position: relative;
    background: #ececec;
    .thirdLeft {
        transform: translate3d(0,10em, 0);
        z-index: 1000;
        position: absolute;
        right: 2em;
        top: 5em;
        width: 33.33%;
        h3 {
            font-size: $fontSize;
            line-height: $fontSize;
            font-weight: 700;
            color: $titleColor;
        }
        p {
            font-size: $txtFontSize;
            color: $txtColor;
            line-height: $fontSize;
            margin-top: 1em;
        }
        hr {
            width: 80%;
            height: 1px;
            border: 0;
            background: #e2e3e4;
            margin-top: 26px;
        }
        div {
            span {
                color: $grayColor;
                font-size: 34px;
                line-height: 40px;
                margin-top: 30px;
                display: block;
                font-weight: 200!important;
            }
            p {
                margin-right: 2em;
            }
        }
    }
    img {
        position: absolute;
        bottom: 0;
        left: 0;
        width: 60%;
        height: 90%;
        transform: translate3d(-10em, 0, 0);
    }
    img.active {
        transition: transform 4s ease-in;
        transform: translate3d(0,0,0);
    }
    .thirdLeft.active {
        transition: opacity 0.4s linear,transform 3s cubic-bezier(0.500, 0.250, 0.585, 1.480);
        transform: translate3d(0,0,0);
    }
}
.fifth {
    background: #fff;
    position: relative;
    overflow: hidden;
    img {
        position: absolute;
        right: -4em;
        top: 0;
        width: auto;
        height: 100%;
        transition: all 2s ease-in;
        &.active {
            transform: translate3d(-3em,0,0);
        }
    }
    .fifthDes {
        z-index: 1000;
        position: absolute;
        top: 8em;
        left: 5em;
        h1 {
            font-size: 2em;
            color: #555;
            opacity: 0;
            transition: all 2s ease-in;
            &.active{
                opacity: 1;
            }
        }
        div {
            margin-top: 2em;
            font-size: 0.8em;
            color: #888;
            width: 20em;
            transition: all 2s ease-in;
            opacity: 0;
            &.active{
                opacity: 1;
            }
        }
    }
}
.seventh {
    position: relative;
    text-align: center;
    background: #000000;
    .seventhVideo {
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        overflow: hidden;
        video {
            height: 100%;
            width: 100%;
        }
    }
    .seventhDes {
        width: 100%;
        height: 100%;
        background: rgba(0,0,0,.3);
        position: absolute;
        left: 0;
        top: 0;
        h1 {
            margin-top: 20%;
            display: block;
            font-size: 1.4em;
            color: $whiteColor;
            margin-bottom: 1em;
        }
        p {
            left: 30%;
            top: 30%;
            color: rgba(255,255,255,.6);
        }
    }
}
.sixthP {
    overflow: hidden;
    background: $whiteColor;
    position: relative;
    .numb1 {
        height: 30px;
        width: 30px;
        line-height: 30px;
        text-align: center;
        font-weight: 700;
        font-size: 16px;
        background: #d7e0dc;
        border-radius: 100%;
    }
    .txtBox {
        z-index: 300;
        width: 100%;
        text-align: center;
        position: absolute;
        top: 5em;
        transform: translate3d(0, 0, 0);
        h3 {
            color: $titleColor;
            font-size: $fontSize;
        }
        p {
            font-size: $txtFontSize;
            color: $txtColor;
            font-family: "微软雅黑";
            margin-top: 1em;
        }
    }
    .txtBox.active {
        transform: translate3d(0, -10em, 0);
        transition: transform 2s ease-in;
    }
    .txtBox.active1 {
        transform: translate3d(0, 0, 0);
        transition: transform 2s ease-in;
    }
    .imgBox {
        z-index: 200;
        width: 25em;
        height: 25em;
        margin: 0 auto;
        position: relative;
    }
    img {
        width: 100%;
        position: absolute;
        top: 3em;
        display: none;
        &.active {
            display: block;
        }
    }
    .sixTxt {
        position: absolute;
        left: 8em;
        bottom: -20em;
        transform: translate3d(0, -10em, 0);
        h3 {
            color: $titleColor;
            font-size: 26px;
            margin-top: 10px;
        }
        p {
            margin-top: 10px;
            font-size: 16px;
            color: $txtColor;
        }
    }
    .sixTxt.active {
        transform: translate3d(0, -20em, 0);
        transition: transform 500ms ease-in;
    }
    .sixTxt.active1 {
        transform: translate3d(0,-36em,0);
        transition: transform 1s ease-in;
    }
    .sixRight {
        z-index: 300;
        position: absolute;
        right: 5em;
        bottom: -15em;
        transform: translate3d(0, -10em, 0);
        h3 {
            color: $titleColor;
            font-size: 26px;
            margin-top: 10px;
        }
        p {
            margin-top: 10px;
            font-size: 16px;
            color: $txtColor;
        }
    }
    .sixRight.active {
        transform: translate3d(0, -30em, 0);
        transition: transform 1s ease-in;
    }
    .sixLast {
        position: absolute;
        bottom: 6em;
        left: 8em;
        transform: translate3d(0, 10em, 0);
        h3 {
            color: $titleColor;
            font-size: 26px;
            margin-top: 10px;
        }
        p {
            margin-top: 10px;
            font-size: 16px;
            color: $txtColor;
        }
    }
    .sixLast.active {
        transform: translate3d(0, 0, 0);
        transition: transform 1s ease-in;
    }
}
.quality-life {
    width: 100%;
    text-align: center;
    background: $whiteColor;
    position: relative;
    .quality-life-word {
        width: 23em;
        position: absolute;
        top: 2.5em;
        left: 50%;
        margin-left: -12em;
        h1 {
            color: $titleColor;
            font-size: $fontSize;
            margin-bottom: 0.4em;
        }
        span,
        p {
            color: $txtColor;
            font-size: $txtFontSize;
            margin-bottom: 0.3em;
        }
    }
    .type {
        width: 100%;
        position: absolute;
        bottom: 0;
        display: -webkit-box;
        display: -webkit-flex;
        display: -ms-flexbox;
        display: flex;
        div {
            -webkit-box-flex: 1;
            -webkit-flex: 1;
            -ms-flex: 1;
            flex: 1;
            /*等分的比例*/
            height: 15em;
            //          border:1px solid $bgColor;
        }
        .chair,
        .loveseat,
        .sofa {
            text-align: center;
            position: relative;
            img {
                width: 362px;
                height: 216px;
            }
            h2 {
                font-size: 1.3em;
                color: $titleColor;
                margin-bottom: 0.2em;
            }
            ul {
                position: absolute;
                bottom: 1.8em;
                left: 50%;
                margin-left: -92px;
            }
            li {
                box-sizing: border-box;
                width: 25px;
                height: 25px;
                margin-left: 10px;
                border-radius: 50%;
                border: 1px solid $whiteColor;
                padding: 3px;
                div {
                    width: 15px;
                    height: 15px;
                    border-radius: 50%;
                    border: 1px solid $lightColor;
                }
            }
            li:nth-child(1) div {
                background: $redColor;
            }
            li:nth-child(2) div {
                background: $blueColor;
            }
            li:nth-child(3) div {
                background: $greenColor;
            }
            li:nth-child(4) div {
                background: $whiteColor;
            }
            li:nth-child(5) div {
                background: $grayColor;
            }
            li.active {
                border-color: $lightColor;
            }
            p,
            span {
                font-size: 0.8em;
                color: $txtColor;
            }
            span {
                margin-top: 2em;
            }
        }
    }
}
.nineBox {
    background: #f2ede4;
    padding: 1em auto;
    text-align: center;
    h3 {
        font-size: $fontSize;
        line-height: $fontSize;
        font-weight: 700;
        color: $titleColor;
    }
    p {
        font-size: $txtFontSize;
        color: $txtColor;
        line-height: $fontSize;
        margin-top: 0.6em;
    }
    .inputBox {
        box-sizing: border-box;
        margin: 1.2em auto;
        width: 16em;
        height: 1.6em;
        border: 1px solid #f2ede4;
        input {
            box-sizing: border-box;
            padding: 0 1em;
            font-size: $txtFontSize;
            width: 100%;
            height: 100%;
            background: #FFFFFF;
            transition: border-color 0.2s cubic-bezier(.25,.46,.45,.94);
        }
    }
    .loginBtn {
        margin: 0 auto;
        width: 6em;
        height: 2em;
        background: #455544;
        color: #fff;
        text-align: center;
        line-height: 2em;
        font-weight: 600;
        font-size: 18px;
        border-radius: 4px;
        cursor: pointer;
    }
    .loginBtn:hover {
        opacity: 0.5;
    }
    hr {
        margin: 1.2em auto;
        width: 16em;
        height: 1px;
        border: 0;
        background: #e2e3e4;
    }
    h4 {
        margin-bottom: 0.6em;
    }
}
